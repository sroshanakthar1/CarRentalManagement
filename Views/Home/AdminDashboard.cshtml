@{
    ViewData["Title"] = "Admin Dashboard";
}
<h2>Admin Dashboard</h2>
<div class="row g-3">
    <div class="col-md-4">
        <div class="card text-bg-primary"><div class="card-body"><h5>Total Cars</h5><h2>@ViewBag.TotalCars</h2></div></div>
    </div>
    <div class="col-md-4">
        <div class="card text-bg-success"><div class="card-body"><h5>Available Cars</h5><h2>@ViewBag.AvailableCars</h2></div></div>
    </div>
    <div class="col-md-4">
        <div class="card text-bg-dark"><div class="card-body"><h5>Total Bookings</h5><h2>@ViewBag.TotalBookings</h2></div></div>
    </div>
</div>